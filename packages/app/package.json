{
  "name": "@spacetraders/app",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "start": "concurrently --restart-tries 1000 --restart-after 1000 \"tsx src/backend/index.ts\"",
    "start:agent": "concurrently --restart-tries 1000 --restart-after 1000 \"tsx src/automation/index.ts\"",
    "jumptable": "tsx src/backend/generate-jump-table.ts",
    "generate": "prisma generate",
    "prepare": "pnpm run generate",
    "dev:agent": "tsnd -r tsconfig-paths/register src/automation/index.ts",
    "dev:backend": "tsnd -r tsconfig-paths/register src/backend/index.ts",
    "dev:build": "esbuild --bundle --format=esm --watch --outfile=public/app.js src/frontend/index.ts",
    "dev:build-ui": "esbuild --bundle --format=esm --watch --outfile=public/index.js src/frontend/react-ui.tsx",
    "dev:build-graph": "esbuild --bundle --format=esm --watch --outfile=public/tradegoods.js src/frontend/tradegoods.tsx",
    "dev:serve": "serve public",
    "dev": "run-p dev:*",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
