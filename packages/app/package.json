{
	"name": "@spacetraders/app",
	"version": "1.0.0",
	"description": "",
	"main": "src/index.ts",
	"scripts": {
		"build": "tsc && resolve-tspaths",
		"start": "concurrently --restart-tries 1000 --restart-after 1000 \"tsx src/backend/index.ts\"",
		"start:agent": "concurrently --restart-tries 1000 --restart-after 1000 \"tsx src/automation/index.ts\"",
		"jumptable": "tsx src/backend/generate-jump-table.ts",
		"test:tsc": "tsc --noEmit",
		"generate": "prisma generate",
		"prepare": "pnpm run generate",
		"dev:textures": "tsx pack-textures.mts",
		"dev:agent": "tsnd -r tsconfig-paths/register --transpile-only src/automation/index.ts",
		"dev:backend": "tsnd -r tsconfig-paths/register --transpile-only src/backend/index.ts",
		"dev:frontend": "vite",
		"dev:routing": "PORT=5001 poetry run python -m flask run -p 5001",
		"dev": "dotenv -e .env.local -- run-p dev:*",
		"local:dev": "SERVER=mylocal run-p dev:*",
		"test": "vitest",
		"prisma:dev": "dotenv -e .env.local -- prisma migrate deploy",
		"prisma:dev:local": "dotenv -e .env.mylocal -- pnpx prisma migrate deploy"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
